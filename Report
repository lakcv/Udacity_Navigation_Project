    "importlib.reload(banna_agent_pd_dqn)\n",
    "from banna_agent_pd_dqn import Agent\n",
    "agent = Agent(state_size=state_size, action_size=action_size, seed=0, pow_alpha = 0.0, pow_betta = 0.0)"
   ]
